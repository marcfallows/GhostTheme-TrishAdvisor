{
  "name": "GhostTheme-TrishAdvisor",
  "version": "0.1.0",
  "devDependencies": {
    "node-sass": "^2.1.1"
  },
  "scripts": {
    "css:build": "node-sass assets/scss/styles.scss --output assets/css",
    "css:watch": "node-sass --watch --recursive assets/scss assets/scss/styles.scss --output assets/css",
    "watch": "npm run css:watch"
  }
}
